{
  "name": "fifa-tournament",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx run-many --target serve --projects=fifa-tournament-fe,fifa-tournament-api --parallel=true",
    "start:fifa-tournament-fe": "nx run fifa-tournament-fe:serve --port 4200",
    "start:fifa-tournament-api": "nx run fifa-tournament-api:serve",
    "build:fifa-tournament-fe": "nx run fifa-tournament-fe:build --configuration=production",
    "e2e:fifa-tournament-fe": "nx e2e fifa-tournament-fe-e2e --watch",
    "e2e:fifa-tournament-components": "nx run fifa-tournament-components-e2e:e2e --watch",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "eslint:all": "npm run eslint:apps:fix && npm run eslint:libs:fix",
    "eslint:apps:fix": "eslint --no-error-on-unmatched-pattern --cache --fix --no-ignore -c ./.eslintrc.js ./apps/**/*{.ts,.tsx,.js,.jsx}",
    "eslint:libs:fix": "eslint --no-error-on-unmatched-pattern --cache --fix --no-ignore -c ./.eslintrc.js ./libs/**/*{.ts,.tsx,.js,.jsx}",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "document-register-element": "1.13.1",
    "express": "4.17.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "5.2.0",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "0.13.7",
    "styled-components": "5.2.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/cli": "12.3.4",
    "@nrwl/cypress": "12.3.4",
    "@nrwl/eslint-plugin-nx": "12.3.4",
    "@nrwl/express": "12.3.4",
    "@nrwl/jest": "12.3.4",
    "@nrwl/linter": "12.3.4",
