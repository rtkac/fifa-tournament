    "@nrwl/react": "12.3.4",
    "@nrwl/storybook": "12.3.4",
    "@nrwl/tao": "12.3.4",
    "@nrwl/web": "12.3.4",
    "@nrwl/workspace": "12.3.4",
    "@storybook/addon-knobs": "^6.2.7",
    "@storybook/react": "^6.2.7",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "11.2.6",
    "@types/express": "4.17.0",
    "@types/jest": "26.0.8",
    "@types/node": "^15.6.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
